{
  "name": "dominator-lx",
  "description": "涉及dom元素监听、信息获取和位移操作等系列功能，可以注册为vue插件，也可以导出hooks使用",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/dominator-lx.umd.cjs",
  "module": "./dist/dominator-lx.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/dominator-lx.js",
      "require": "./dist/dominator-lx.umd.cjs"
    }
  },
  "dependencies": {
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/react.git"
  },
  "license": "MIT",
  "author": "lxw"
}
