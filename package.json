{
  "name": "vue-dominator",
  "description": "涉及dom元素监听、信息获取和位移操作等系列功能，可以注册为vue插件，也可以导出hooks使用",
  "private": false,
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/vue-dominator.umd.cjs",
  "module": "./dist/vue-dominator.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-dominator.js",
      "require": "./dist/vue-dominator.umd.cjs"
    }
  },
  "dependencies": {
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.2",
    "@types/node": "^20.11.26",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "happy-dom": "^13.8.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{ts,vue,tsx}": [
      "vitest related --run"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lxw-bfw/vue-dominator-lx"
  },
  "license": "MIT",
  "author": "lxw"
}
